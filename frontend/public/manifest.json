{
    "manifest_version": 3,
    "name": "tusk.ai",
    "version": "1.0.0",
    "description": "Extract and process data from web pages",
    "permissions": [
      "activeTab",
      "scripting",
      "storage"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_title": "tusk.ai"
    },
    "background": {
      "scripts": ["background.js"]
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": ["../log.html", "*.js", "*.css"],
        "matches": ["<all_urls>"]
      }
    ]
  }
  
